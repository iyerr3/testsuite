                        <value>False</value>
                    </list_parameter>
                    <list_parameter>
                        <name>get_estimates</name>
                        <value>True</value>
                        <value>False</value>
                    </list_parameter>
    	        </parameters>
	    	    <tear_down>
                    DROP TABLE IF EXISTS madlibtestresult.summary_output
                </tear_down>
	        </method>
        </test_suite>
