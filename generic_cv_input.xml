<?xml version='1.0' encoding='UTF-8'?>
<test_suites>
    <test_value>feature</test_value>
    <multi_test_suites>
        <algorithm>generic_cv</algorithm>
        <methods>
                <method>
                    <name>cross_validation_general</name>
                    <parameter>
                        <name>benchmark</name>
                        <value>TemplateExecutor</value>
                    </parameter>
                </method>
        </methods>

        <test_suite>
            <name>cross_validation_null_input_linregr</name>
            <comments>
                Testing invalid values of input for cross validation function
            </comments>
            <!-- It indicate max execution times-->
            <execute_rate>1</execute_rate>
            <method>
                <name>cross_validation_general</name>
                <list_parameter>
                    <name>modelling_func</name>
                    <value>NULL</value>
                    <value>madlib.linregr</value>
                    <value>madlib.logregr</value>
                </list_parameter>
                <list_parameter>
                    <name>modelling_params</name>
                    <value>NULL</value>
                    <value>{NULL}</value>
                    <value>{}</value>
                </list_parameter>
                <input_parameter>
                    <name>modelling_params_type</name>
                    <value>TEXT[]</value>
                </input_parameter>
                <input_parameter>
                    <name>param_explored</name>
                    <value>TEXT</value>
                </input_parameter>
                <input_parameter>
                    <name>explore_values</name>
                    <value>TEXT[]</value>
                </input_parameter>
                <input_parameter>
                    <name>predict_func</name>
                    <value>TEXT</value>
                </input_parameter>
                <input_parameter>
                    <name>predict_params</name>
                    <value>TEXT[]</value>
                </input_parameter>
                <input_parameter>
                    <name>predict_params_type</name>
                    <value>TEXT[]</value>
                </input_parameter>
                <input_parameter>
                    <name>metric_func</name>
                    <value>TEXT</value>
                </input_parameter>
                <input_parameter>
                    <name>metric_params</name>
                    <value>TEXT[]</value>
                </input_parameter>
                <input_parameter>
                    <name>metric_params_value</name>
                    <value>TEXT[]</value>
                </input_parameter>
                <input_parameter>
                    <name>data_tbl</name>
                    <value>TEXT</value>
                </input_parameter>
                <input_parameter>
                    <name>data_id</name>
                    <value>TEXT</value>
                </input_parameter>
                <input_parameter>
                    <name>id_is_random</name>
                    <value>BOOLEAN</value>
                    <quote>false</quote>
                </input_parameter>
                <input_parameter>
                    <name>validation_result</name>
                    <value>TEXT</value>
                </input_parameter>
                <input_parameter>
                    <name>fold_num</name>
                    <value>DOUBLE PRECISION</value>
                    <quote>false</quote>
                </input_parameter>
                <input_parameter>
                    <name>upto_fold</name>
                    <value>INTEGER</value>
                    <quote>false</quote>
                </input_parameter>
                <input_parameter>
                    <name>data_cols</name>
                    <value>TEXT[]</value>
                </input_parameter>
            </method>
        </test_suite>
    </multi_test_suites>
</test_suites>
